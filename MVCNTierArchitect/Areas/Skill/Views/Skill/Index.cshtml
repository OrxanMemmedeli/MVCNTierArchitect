    <!--//web font-->
    <!--circle-chart-->
    <script src="~/Content/Skills_Theme/js/jquery-1.11.1.min.js"></script>
    <!--//circle-chart-->
    <!--ResponsiveTabs-->
    <script src="~/Content/Skills_Theme/js/easyResponsiveTabs.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#horizontalTab').easyResponsiveTabs({
                type: 'default', //Types: default, vertical, accordion
                width: 'auto', //auto or any width like 600px
                fit: true   // 100% fit in a container
            });
        });
    </script>
    <!--//ResponsiveTabs-->
</head>
<body>
    <!-- main -->
    <div class="main">
        <h1>Bacarıqlar Kartı</h1>
        <div class="main-info w3l">
            <div class="main-row">
                <!-- main-row-one -->
                @Html.Action("About", "Skill", "Skill")
                <h3 class="title">Bacarıqlar</h3>
                @Html.Action("Skills", "Skill", "Skill")
