                <div class="clear"> </div>
            </div>
        </div>
        <!-- copyright -->
        <div class="copyright">
            <p> <a href="/Admin" target="_blank">Geri</a></p>
            <p> <a href="/Skill/Skill/Details" target="_blank">Məlumatları Dəyiş</a></p>
